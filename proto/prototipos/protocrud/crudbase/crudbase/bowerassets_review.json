{
  "name": "procon_sistema_integrado",
  "authors": [
    "Carlos Melo <carlosmelo@sp.gov.br>"
  ],
  "description": "Referencia para instalacao de pacotes frontend Sistema Integrado",
  "version": "0.1",
  "main": "bower.json",
  "moduleType": [],
  "license": "MIT",
  "homepage": "https://portal.procon.sp.gov.br",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "2.1.4",
    "jquery.validation": "1.11.1",
    "Microsoft.jQuery.Unobtrusive.Validation": "3.2.3",
    "modernizr": "2.6.2",
    "bootstrap": "3.3.5",
    "jquery-mask-plugin": "1.14.10",
    "jquery.maskedinput": "1.4.1",
    "smalot-bootstrap-datetimepicker": "2.0.0",
    "notifyjs": "0.4.2",
    "font-awesome": "4.7.0",
    "bs-typeahead": "0.0.5",
    "moment": "2.22.1",
    "datatables": "1.10.16",
    "datatables-responsive": "2.2.1",
    "bootbox.js": "4.4.0",
    "bootstrap-modal": "2.2.6",
    "offline": "0.7.18",
    "bootstrap-tagsinput":"0.8.0",
    "bootstrap-duallistbox":"3.0.5"
  },
  "install": {
    "options": {
      "includePackageNameInInstallPath": false
    },
    "base": "assets_review",
    "path": {
      "js": "scripts/{name}",
      "css": "css/{name}",
      "map": "css/{name}",
      "eot": "css/fonts",
      "svg": "css/fonts",
      "ttf": "css/fonts",
      "woff": "css/fonts",
      "woff2": "css/fonts",
      "otf": "css/fonts",
      "png": "css/images",
      "psd": "css/images",
      "gif": "css/images"
    },
    "sources": {
      "jquery": [
        {
          "bower_components/jquery/dist/jquery.js": "jquery.js"
        },
        {
          "bower_components/jquery/dist/jquery.min.js": "jquery.min.js"
        }
      ],
      "jquery.validation": [
        "bower_components/jquery.validation/jquery.validate.js"
      ],
      "Microsoft.jQuery.Unobtrusive.Validation": [
        "bower_components/Microsoft.jQuery.Unobtrusive.Validation/jquery.validate.unobtrusive.js",
        "bower_components/Microsoft.jQuery.Unobtrusive.Validation/jquery.validate.unobtrusive.min.js"
      ],
      "modernizr": [
        "bower_components/modernizr/modernizr.js"
      ],
      "bootstrap": [
        "bower_components/bootstrap/dist/css/bootstrap.css",
        "bower_components/bootstrap/dist/css/bootstrap.css.map",
        "bower_components/bootstrap/dist/css/bootstrap.min.css",
        "bower_components/bootstrap/dist/css/bootstrap-theme.css",
        "bower_components/bootstrap/dist/css/bootstrap-theme.css.map",
